<tr align="center">
  <th scope="row"><%= user.id %></th>
  <td><%= user.name %></td>
  <td><%= user.username %></td>
  <td><%= user.email %></td>
  <td><%= user.role %></td>
  <td><%= l user.created_at, format: :short %></td>
  <td>

    <div class="btn-group"></div>
      <%= link_to t('global.link.edit'), edit_admin_user_path(user), class: 'button button-dark button-small' %>
    </div>
    <div class="btn-group">
      <%= link_to t('global.link.delete'), admin_user_path(user), class: 'button submit-danger button-small',
          data: {method: :delete, confirm: t('global.dialog.you_sure')} %>
    </div>

  </td>
</tr>
